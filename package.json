{
  "name": "quorum-creator",
  "version": "0.1.2",
  "description": "Tool for walking through the setup of Quorum networks.",
  "main": "src/index.js",
  "bin": {
    "quorum-creator": "build/index.js"
  },
  "preferGlobal": true,
  "files": [
    "build",
    "lib",
    "7nodes",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "babel src -d build --ignore \"src/**/*.test.js\" --source-maps inline",
    "start": "babel src --watch -d build --ignore \"src/**/*.test.js\"  --source-maps inline",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.js\"",
    "fix": "eslint --fix \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QuorumEngineering/quorum-creator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QuorumEngineering/quorum-creator/issues"
  },
  "homepage": "https://github.com/QuorumEngineering/quorum-creator#readme",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "axios": "^0.19.2",
    "enquirer": "^2.3.4",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "merge-files": "^0.1.2",
    "react": "^16.12.0",
    "sanitize-filename": "^1.6.3",
    "source-map-support": "^0.5.16",
    "tar-fs": "^2.0.0",
    "web3": "^1.2.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-preset-airbnb": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "expect": "latest"
  }
}
